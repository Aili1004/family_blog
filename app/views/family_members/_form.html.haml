.form-div
  = form_for(@family_member) do |f|
    - if @family_member.errors.any?
      #error_explanation
        %h2
          = pluralize(@family_member.errors.count, "error")
          prohibited this family_member from being saved:
        %ul
          - @family_member.errors.full_messages.each do |message|
            %li= message
    .field
      = f.label :first_name
      %br/
      = f.text_field :first_name
    .field
      = f.label :last_name
      %br/
      = f.text_field :last_name
    .field
      = f.label :dob
      %br/
      = f.date_field :dob
    .field
      = f.label :email
      %br/
      = f.text_area :email
    .field
      = f.label :image
      %br/
      = f.text_area :image
    .field
      = f.label :occupation
      %br/
      = f.text_field :occupation
    .field
      = f.label :description
      %br/
      = f.text_area :description
    .field
      = f.label :phone_number
      %br/
      = f.text_field :phone_number
    .actions
      = f.submit
