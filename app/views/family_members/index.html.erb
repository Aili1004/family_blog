<p id="notice"><%= notice %></p>

<table>
  <thead>
    <tr>
      <th></th>
      <th>First name</th>
      <th>Last name</th>
      <th>Dob</th>
      <th>Occupation</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @family_members.each do |family_member| %>
      <tr>
        <td><%= image_tag family_member.image, class: "profile" %></td>

        <td><%= family_member.first_name %></td>
        <td><%= family_member.last_name %></td>
        <td><%= family_member.dob %></td>
        <td><%= family_member.occupation %></td>
        <td><%= link_to 'View Details', family_member %></td>
      </tr>
    <% end %>
  </tbody>
</table>

